{
  "manifest_version": 3,
  "name": "YT shortcut",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "popup.js"
      ]
    }
  ]
}